@model IEnumerable<GenshinCourse.batch>

    @{
    ViewBag.Title = "Index";
    }

    <h2>Daftar List Batch</h2>

    <p>
        @if(Session["UserId"] != null)
{
        @Html.ActionLink("Create New", "Create")
}else{
}
    </p>
    <table class="table">
        <tr style="background-color: #E1F4F3">
            <th style="text-align: center;" width="50%">
                <!--@Html.DisplayNameFor(model => model.batch1)-->
                Batch
            </th>
            <th style="text-align: center;">
                <!--@Html.DisplayNameFor(model => model.year)-->
                Year
            </th>
            @if(Session["UserId"] != null)
        {
            <th style="text-align: center;">
                Set
            </th>
        }

        </tr>

        @foreach (var item in Model) {
        <tr style="text-align: center;">
            <td class="table-info">
                @Html.DisplayFor(modelItem => item.batch1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.year)
            </td>
            @if(Session["UserId"] != null)
    {
            <td style="text-align: center;">
                <a href=@Url.Action("Edit", "Batches", new { id=item.id }) class="btn btn-warning">Edit</a>
                <a href=@Url.Action("Details", "Batches", new { id=item.id }) class="btn btn-success">Details</a>
                <a href=@Url.Action("Delete", "Batches", new { id=item.id }) class="btn btn-danger">Delete</a>
            </td>
    }
        </tr>
}

    </table>
